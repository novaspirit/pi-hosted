{
	"categories": [
		"Downloaders",
		"Video"
	],
	"description": "Radarr - A fork of Sonarr to work with movies Ã  la Couchpotato.",
	"env": [
		{
			"default": "1000",
			"label": "PUID",
			"name": "PUID"
		},
		{
			"default": "1000",
			"label": "PGID",
			"name": "PGID"
		}
	],
	"image_arm64": "linuxserver/radarr:latest",
	"image_amd64": "linuxserver/radarr:latest",
