	"name": "radarr",
	"officialDoc": "https://docs.linuxserver.io/images/docker-radarr",
	"platform": "linux",
	"ports": [
		"7878:7878/tcp"
	],
	"restart_policy": "unless-stopped",
	"title": "Radarr",
